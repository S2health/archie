definition
	Composition[id1] matches {	-- Encounter
		code matches {
			Terminology_term[id10] matches {
				concept matches {[at1]}
			}
		}
		category matches {
			Terminology_term[id11] matches {
				concept matches {[at2]}
				description matches {"persistent"}
			}
		}
		context matches {
			Event_context[id2] occurrences matches {1} matches {
				other_context matches {
					Node[id3] occurrences matches {0..1} matches {	-- Encounter Id
						value matches {
							Rwe_id_ref[id4] 
						}
					}
				}
			}
		}
	}

