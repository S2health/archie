definition
    Admin_entry[id1] matches {    -- Three Delays Model (3DM)
        data matches {
            Node[id6] occurrences matches {0..1} matches {    -- Delay category
                value matches {
                    Coded_text[id25] matches {
                        term matches {
                            Terminology_term[id32] matches {
                                concept matches {[ac1]}    -- Delay category (synthesised)
                            }
                        }
                    }
                    Plain_text[id26]
                }
            }
            Node[id14] occurrences matches {0..1} matches {    -- Delay presence?
                value matches {
                    Coded_text[id27] matches {
                        term matches {
                            Terminology_term[id33] matches {
                                concept matches {[ac2]}    -- Delay presence? (synthesised)
                            }
                        }
                    }
                }
            }
            use_archetype Node[id13, s2-EHR-Node.delay_details.v0]    -- Delay details
            Node[id9] occurrences matches {0..1} matches {    -- Description
                value matches {
                    Text[id28]
                }
            }
            use_archetype Node[id23, s2-EHR-Node.person.v1] occurrences matches {0..1}     -- Solution responsibility
            Node[id24] occurrences matches {0..1} matches {    -- Solution timeline
                value matches {
                    Date_time_value[id29]
                    Duration_value[id30] matches {
                        magnitude matches {|>=PT0S|}
                    }
                }
            }
            Node[id10] occurrences matches {0..1} matches {    -- Comment
                value matches {
                    Text[id31]
                }
            }
        }
    }

