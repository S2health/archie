definition
	Assessment[id1] matches {	-- Occupation summary
		data matches {
			Node[id3] occurrences matches {0..1} matches {	-- Description
				value matches {
					Text[id11] 
				}
			}
			Node[id5] matches {	-- Employment status
				value matches {
					Text[id12] 
				}
			}
			use_archetype Node[id4, s2-EHR-Node.occupation_record.v1] 
			allow_archetype Node[id6] matches {	-- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			Node[id7] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id13] 
				}
			}
		}
		qualifiers matches {
			allow_archetype Node[id9] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
			Node[id10] occurrences matches {0..1} matches {	-- Last updated
				value matches {
					Date_time_value[id14] 
				}
			}
		}
	}

