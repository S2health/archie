	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"To record summary details of exposure of the subject to a chemical, physical or biological agent within their environment that has caused, or may possibly cause in the future, a negative impact on health.">
			use = <"Use to record summary details of exposure of the subject to a chemical, physical or biological agent within their environment that has caused, or may possibly cause in the future, a negative impact on health.
            
            This archetype has been designed primarily to capture a simple amount of details about exposure, fitting current requirements for NT Hearing Health exposure to passive smoking, campfire smoke and noise. However additional research should be conducted to enhance this archetype to cater for the detailed occupation exposure summaries etc.">
			keywords = <"substance", "chemical", "smoke", "passive", "noise", "pollution", "chemical">
			misuse = <"">
		>
	>

definition
	Assessment[id1] matches {	-- Exposure
		data matches {
			Node[id3] occurrences matches {0..1} matches {	-- Agent
				value matches {
					Text[id15] 
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Overall Description
				value matches {
					Text[id16] 
				}
			}
			Node[id6] occurrences matches {0..1} matches {	-- Category
				value matches {
					Coded_text[id17] matches {
						term matches {
							Terminology_term[id23] matches {
								concept matches {[ac1]}		-- Category (synthesised)
							}
						}
					}
				}
			}
			Node[id5] occurrences matches {0..1} matches {	-- Ongoing Exposure
				value matches {
					Boolean_value[id18] matches {
						value matches {True}
					}
				}
			}
			Node[id11] matches {	-- Exposure Details
				items matches {
					Node[id12] occurrences matches {0..1} matches {	-- Date of Onset
						value matches {
							Date_time_value[id19] 
						}
					}
					Node[id14] occurrences matches {0..1} matches {	-- Description
						value matches {
							Text[id20] 
						}
					}
					Node[id13] occurrences matches {0..1} matches {	-- Date Ceased
						value matches {
							Date_time_value[id21] 
						}
					}
				}
			}
		}
		qualifiers matches {
			Node[id10] occurrences matches {0..1} matches {	-- Date Updated
				value matches {
					Date_time_value[id22] 
				}
			}
		}
	}

