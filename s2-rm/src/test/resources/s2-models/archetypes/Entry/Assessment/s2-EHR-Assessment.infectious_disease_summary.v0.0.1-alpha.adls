definition
	Assessment[id1] matches {	-- Infectious disease summary
		data cardinality matches {1..*; unordered} matches {
			Node[id3] matches {	-- Infectious disease
				value matches {
					Text[id53] 
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Immune status
				value matches {
					Text[id54] 
				}
			}
			Node[id43] occurrences matches {0..1} matches {	-- Description of immune status
				value matches {
					Text[id55] 
				}
			}
			Node[id48] occurrences matches {0..1} matches {	-- Previous infection?
				value matches {
					Coded_text[id56] matches {
						term matches {
							Terminology_term[id59] matches {
								concept matches {[ac1]}		-- Previous infection? (synthesised)
							}
						}
					}
				}
			}
			allow_archetype Node[id47] matches {	-- Evidence
				include
					archetype_id/value matches {/.*/}
			}
			Node[id15] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id57] 
				}
			}
		}
		qualifiers matches {
			Node[id22] occurrences matches {0..1} matches {	-- Last Updated
				value matches {
					Date_time_value[id58] 
				}
			}
			allow_archetype Node[id52] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

