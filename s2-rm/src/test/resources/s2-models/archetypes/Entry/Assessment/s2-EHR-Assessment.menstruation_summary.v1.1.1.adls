definition
	Assessment[id1] matches {	-- Menstruation summary
		data matches {
			Node[id26] occurrences matches {0..1} matches {	-- Overall description
				value matches {
					Text[id27] 
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Menstrual status
				value matches {
					Coded_text[id28] matches {
						term matches {
							Terminology_term[id49] matches {
								concept matches {[ac1]}		-- Menstrual status (synthesised)
							}
						}
					}
				}
			}
			Node[id3] occurrences matches {0..2} matches {	-- Menarche
				value matches {
					Duration_value[id29] matches {
						magnitude matches {|>=PT0S|}
					}
					Date_value[id30] 
				}
			}
			Node[id11] matches {	-- Per episode
				items matches {
					Node[id12] occurrences matches {0..1} matches {	-- Episode label
						value matches {
							Text[id31] 
						}
					}
					Node[id13] occurrences matches {0..1} matches {	-- Episode start date
						value matches {
							Date_time_value[id32] 
						}
					}
					Node[id14] occurrences matches {0..1} matches {	-- Description
						value matches {
							Text[id33] 
						}
					}
					Node[id15] occurrences matches {0..1} matches {	-- Typical pattern
						value matches {
							Coded_text[id34] matches {
								term matches {
									Terminology_term[id50] matches {
										concept matches {[ac2]}		-- Typical pattern (synthesised)
									}
								}
							}
						}
					}
					Node[id18] occurrences matches {0..1} matches {	-- Typical cycle length
						value matches {
							Duration_value[id35] matches {
								magnitude matches {|P1D..P999D|}
							}
							Range<Duration_value>[id36] matches {
								lower matches {
									Duration_value[id37] matches {
										magnitude matches {|P1D..P999D|}
									}
								}
								upper matches {
									Duration_value[id38] matches {
										magnitude matches {|P1D..P999D|}
									}
								}
							}
						}
					}
					Node[id19] occurrences matches {0..1} matches {	-- Typical menses duration
						value matches {
							Duration_value[id39] matches {
								magnitude matches {|P1D..P999D|}
							}
							Range<Duration_value>[id40] matches {
								lower matches {
									Duration_value[id41] matches {
										magnitude matches {|P1D..P999D|}
									}
								}
								upper matches {
									Duration_value[id42] matches {
										magnitude matches {|P1D..P999D|}
									}
								}
							}
						}
					}
					allow_archetype Node[id21] matches {	-- Additional details
						include
							archetype_id/value matches {/.*/}
					}
					Node[id22] occurrences matches {0..1} matches {	-- Episode end date
						value matches {
							Date_time_value[id43] 
						}
					}
					Node[id23] occurrences matches {0..1} matches {	-- Episode comment
						value matches {
							Text[id44] 
						}
					}
				}
			}
			Node[id5] occurrences matches {0..2} matches {	-- Menopause
				value matches {
					Duration_value[id45] matches {
						magnitude matches {|>=PT0S|}
					}
					Date_value[id46] 
				}
			}
			Node[id24] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id47] 
				}
			}
		}
		qualifiers matches {
			allow_archetype Node[id10] matches {	-- Extension
				include
