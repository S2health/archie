	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"To record summary or persistent information about an individual's menstruation history events or patterns of menstruation over time.">
			use = <"Use to record summary or persistent information about an individual's menstruation history events or patterns of menstruation over time.
            
            Multiple instances of the 'Per episode' Node will allow aggregation of an evolving history of both past and present episodes, or patterns, of menstruation.
            
            The 'Last updated' data Node will record the last time that the Menstruation summary as a whole, including each episode, was updated.
            
            Use to incorporate the narrative descriptions of Menstruation or menstrual history within existing or legacy clinical systems into an archetyped format, using the 'Overall description' data Node.">
			keywords = <"menstruation", "menarche", "menopause", "menopausal", "perimenopausal", "postmenopausal">
			misuse = <"Not to be used to record information about a specific menstrual cycle. Use OBSERVATION.menstruation for this purpose.">
		>
	>
