definition
    Assessment[id1] matches {    -- Estimated date of delivery
        data matches {
            Node[id4] occurrences matches {0..1} matches {    -- By date of conception
                value matches {
                    Date_time_value[id15]
                }
            }
            Node[id5] occurrences matches {0..1} matches {    -- By cycle
                value matches {
                    Date_time_value[id16]
                }
            }
            Node[id7] occurrences matches {0..1} matches {    -- By ultrasound
                items matches {
                    Node[id6] occurrences matches {0..1} matches {    -- Date of ultrasound
                        value matches {
                            Date_time_value[id17]
                        }
                    }
                    Node[id8] occurrences matches {0..1} matches {    -- Gestation by scan
                        value matches {
                            Duration_value[id18]
                        }
                    }
                    Node[id9] matches {    -- Estimated date by ultrasound
                        value matches {
                            Date_time_value[id19]
                        }
                    }
                }
            }
            Node[id10] matches {    -- Agreed EDD
                items matches {
                    Node[id11] occurrences matches {0..1} matches {    -- Agreed date
                        value matches {
                            Date_time_value[id20]
                        }
                    }
                    Node[id12] occurrences matches {0..1} matches {    -- Rationale
                        value matches {
                            Text[id21]
                        }
                    }
                }
            }
        }
        qualifiers matches {
            Node[id14] occurrences matches {0..1} matches {    -- Last updated
                value matches {
                    Date_time_value[id22]
                }
            }
            allow_archetype Node[id13] matches {     -- Extension
                include
                    archetype_id/value matches {/.*/}
            }
        }
    }

