			}
			Node[id4] occurrences matches {0..1} matches {	-- Age started
				value matches {
					Duration_value[id34] matches {
						magnitude matches {|>=PT0S|}
					}
				}
			}
			Node[id32] occurrences matches {0..1} matches {	-- Age ended
				value matches {
					Duration_value[id35] matches {
						magnitude matches {|>=PT0S|}
					}
				}
			}
			Node[id3] occurrences matches {0..1} matches {	-- Highest level completed
				value matches {
					Text[id36] 
				}
			}
			allow_archetype Node[id30] matches {	-- Education record
				include
					archetype_id/value matches {/.*/}
			}
			allow_archetype Node[id31] matches {	-- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			Node[id8] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id37] 
				}
			}
		}
		qualifiers matches {
			allow_archetype Node[id28] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
			Node[id29] occurrences matches {0..1} matches {	-- Last updated
				value matches {
					Date_time_value[id38] 
				}
			}
		}
	}

