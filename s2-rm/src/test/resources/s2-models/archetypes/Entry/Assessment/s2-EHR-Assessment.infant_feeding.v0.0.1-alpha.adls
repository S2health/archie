						term matches {
							Terminology_term[id35] matches {
								concept matches {[ac1]}		-- Overall feeding type (synthesised)
							}
						}
					}
					Plain_text[id25] 
				}
			}
			Node[id4] matches {	-- Feeding episode
				items matches {
					Node[id7] occurrences matches {0..1} matches {	-- Type
						value matches {
							Coded_text[id26] matches {
								term matches {
									Terminology_term[id36] matches {
										concept matches {[ac2]}		-- Type (synthesised)
									}
								}
							}
							Plain_text[id27] 
						}
					}
					Node[id5] occurrences matches {0..1} matches {	-- Age Commenced
						value matches {
							Duration_value[id28] matches {
								magnitude matches {|>=PT0S|}
							}
						}
					}
					Node[id6] occurrences matches {0..1} matches {	-- Age Ceased
						value matches {
							Duration_value[id29] 
						}
					}
					Node[id10] occurrences matches {0..1} matches {	-- Comment
						value matches {
							Text[id30] 
						}
					}
				}
			}
			Node[id8] occurrences matches {0..1} matches {	-- Total Duration of Breast Feeding
				value matches {
					Duration_value[id31] matches {
						magnitude matches {PYMWD}
					}
				}
			}
			Node[id9] occurrences matches {0..1} matches {	-- Age Commenced Solid Foods
				value matches {
					Duration_value[id32] matches {
						magnitude matches {PMWD}
					}
				}
			}
			Node[id11] occurrences matches {0..1} matches {	-- Age Weaned
				value matches {
					Duration_value[id33] matches {
						magnitude matches {PYM}
					}
				}
			}
			Node[id20] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id34] 
				}
			}
		}
