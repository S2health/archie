definition
	Assessment[id1] matches {	-- Communication capability
		data matches {
			Node[id3] occurrences matches {0..1} matches {	-- Description
				value matches {
					Text[id24] 
				}
			}
			Node[id16] matches {	-- Per language
				name matches {"Preferred language", "Alternative language"}
				items matches {
					use_archetype Node[id4, s2-EHR-Node.language.v1] 
					allow_archetype Node[id19] matches {	-- Capability details
						include
							archetype_id/value matches {/.*/}
					}
					Node[id22] occurrences matches {0..1} matches {	-- Comment
						value matches {
							Text[id26] 
						}
					}
				}
			}
			Node[id23] matches {	-- Communication aid
				value matches {
					Text[id27] 
				}
			}
			allow_archetype Node[id6] occurrences matches {0..1} matches {	-- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			Node[id10] occurrences matches {0..1} matches {	-- Overall comment
				value matches {
					Text[id28] 
				}
			}
		}
		qualifiers matches {
			allow_archetype Node[id13] occurrences matches {0..1} matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
			Node[id14] occurrences matches {0..1} matches {	-- Last updated
				value matches {
					Date_time_value[id29] 
				}
			}
		}
	}

