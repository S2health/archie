definition
	Assessment[id1] matches {	-- Birth summary
		data matches {
			Node[id5] occurrences matches {0..1} matches {	-- Date/time of birth (DOB)
				value matches {
					Date_time_value[id16] 
				}
			}
			use_archetype Node[id4, s2-EHR-Node.dob_alternative.v0] 
			Node[id3] occurrences matches {0..1} matches {	-- Country of birth
				value matches {
					Text[id17] 
				}
			}
			Node[id13] occurrences matches {0..1} matches {	-- Place of birth
				value matches {
					Text[id18] 
				}
			}
			allow_archetype Node[id6] matches {	-- Structured place of birth
				include
					archetype_id/value matches {/s2-EHR-Node\.organization(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.address(-[a-zA-Z0-9_]+)*\.v1/}
			}
			Node[id7] occurrences matches {0..1} matches {	-- Pregnancy/birth summary
				value matches {
					Text[id19] 
				}
			}
			Node[id8] occurrences matches {0..1} matches {	-- Birth plurality
				value matches {
					Text[id20] 
				}
			}
			use_archetype Node[id9, s2-EHR-Node.birth_detail.v0] 
			allow_archetype Node[id14] matches {	-- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			Node[id15] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id21] 
				}
			}
		}
		qualifiers matches {
			Node[id11] occurrences matches {0..1} matches {	-- Last updated
				value matches {
					Date_time_value[id22] 
				}
			}
			allow_archetype Node[id12] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

