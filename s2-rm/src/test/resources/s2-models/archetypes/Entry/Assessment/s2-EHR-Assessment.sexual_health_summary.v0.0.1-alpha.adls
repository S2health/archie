definition
	Assessment[id1] matches {	-- Sexual health summary
		data matches {
			Node[id13] occurrences matches {0..1} matches {	-- Description
				value matches {
					Text[id17] 
				}
			}
			Node[id3] occurrences matches {0..1} matches {	-- Status
				value matches {
					Coded_text[id18] matches {
						term matches {
							Terminology_term[id22] matches {
								concept matches {[ac1]}		-- Status (synthesised)
							}
						}
					}
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- First ever sex
				value matches {
					Duration_value[id19] matches {
						magnitude matches {|>=PT0S|}
					}
				}
			}
			Node[id16] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id20] 
				}
			}
		}
		qualifiers matches {
			Node[id5] occurrences matches {0..1} matches {	-- Last updated
				value matches {
					Date_time_value[id21] 
				}
			}
			allow_archetype Node[id15] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

