	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"To record details about parameters plotted as percentiles on a growth chart.">
			use = <"Use to record details about parameters plotted as percentiles on a growth chart.
            
            Use the URI to explicitly link the original measurement as recorded.
            
            Use in a template alongside OBSERVATION.age to record the adjusted age of an infant as part of monitoring child growth patterns.">
			keywords = <"growth", "calculation", "centile", "percentile", "z-score", "SD", "height", "weight", "head cicrumference">
			misuse = <"Not to be used to record actual measurements. Use appropriate OBSERVATION archetypes for this purpose - for example OBSERVATION.height, OBSERVATION.weight, OBSERVATION.head_circumference, OBSERVATION.body_segment or OBSERVATION.blood_pressure.">
		>
	>
	other_contributors = <"Heather Leslie, Ocean Informatics, Australia", "Sam Heard, Ocean Informatics, Australia">

definition
	Direct_observation[id1] matches {	-- Child growth indicators
		data matches {
			Node[id15] occurrences matches {0..1} matches {	-- Growth indicator
				value matches {
					Coded_text[id43] matches {
						term matches {
							Terminology_term[id49] matches {
								concept matches {[ac1]}		-- Growth indicator (synthesised)
							}
						}
					}
				}
			}
			Node[id36] occurrences matches {0..1} matches {	-- URI to original measurement
				value matches {
					Uri_ref[id44] 
				}
			}
			Node[id11] occurrences matches {0..1} matches {	-- Percentile
				value matches {
					Measured<Quantity>[id52] matches {
						value matches {
							Quantity[id45] matches {
								magnitude matches {|0.0..100.0|}
								units matches {
									Coded_text[id51] matches {
										term matches {
											Terminology_term[id50] matches {
												concept matches {[at42]}
											}
										}
									}
								}
							}
						}
					}
				}
			}
			Node[id12] occurrences matches {0..1} matches {	-- Z-score
				value matches {
					Measured<Quantity>[id53] matches {
						value matches {
							Quantity[id46] matches {
								units matches {
									Coded_text[id55] matches {
										term matches {
											Terminology_term[id54] matches {
												concept matches {[at43]}
											}
										}
									}
								}
							}
						}
					}
				}
			}
			Node[id42] occurrences matches {0..1} matches {	-- Clinical interpretation
				value matches {
					Text[id47] 
				}
			}
			Node[id20] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id48] 
				}
			}
		}
		qualifiers matches {
			allow_archetype Node[id33] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

