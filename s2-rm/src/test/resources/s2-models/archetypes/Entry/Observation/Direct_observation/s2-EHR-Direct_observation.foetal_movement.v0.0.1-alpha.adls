    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record the presence and pattern of any and all spontaneous foetal movements in utero, as perceived by the mother.">
            keywords = <"fetal", "kicks", "movements", "flutters", "hits", "baby", "foetus">
            use = <"Use to record evidence of foetal activity as an indirect indicator of foetal well-being.
            
            Use to record the presence of foetal movements and pattern as part of routine antenatal visits from second trimester of pregnancy onwards.
            
            Use to record details of the number of foetal movements felt over a specified time interval, and represented in a 'Kick Chart', most often in the third trimester.">
            misuse = <"Not to be used to record the response of a foetus to deliberate stimulation eg vibroacoustic stimulation.
            
            Not to be used to represent formal foetal heart rate monitoring eg using cardiotocographs.">
        >
    >
    
definition
    Direct_observation[id1] matches {    -- Foetal movement
        data matches {
            Node[id5] occurrences matches {0..1} matches {    -- Presence
                value matches {
                    Coded_text[id30] matches {
                        term matches {
                            Terminology_term[id37] matches {
                                concept matches {[ac1]}    -- Presence (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id20] occurrences matches {0..1} matches {    -- Pattern
                value matches {
                    Coded_text[id31] matches {
                        term matches {
                            Terminology_term[id38] matches {
                                concept matches {[ac2]}    -- Pattern (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id7] occurrences matches {0..1} matches {    -- Movements
                value matches {
                    Count[id32] matches {
                        magnitude matches {|>=0|}
                    }
                }
            }
            Node[id26] occurrences matches {0..1} matches {    -- Time since last movement
                value matches {
                    Duration_value[id33] matches {
                        magnitude matches {|>=PT0S|}
                    }
                }
            }
            Node[id29] occurrences matches {0..1} matches {    -- Description
                value matches {
                    Text[id34]
                }
            }
        }
        data_series matches {
            Interval_event[id28] occurrences matches {0..1} matches {    -- Period of observation
                sample_function matches {"total"}
                items matches {
                    use_node Node[id5] /data[id5]    -- Presence
                    use_node Node[id20] /data[id20]    -- Pattern
                    use_node Node[id7] /data[id7]    -- Movements
                    use_node Node[id26] /data[id26]    -- Time since last movement
                    use_node Node[id29] /data[id29]    -- Description
                }
            }
        }
    }

