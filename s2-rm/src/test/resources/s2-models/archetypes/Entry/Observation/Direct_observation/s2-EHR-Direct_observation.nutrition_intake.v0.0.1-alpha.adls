definition
	Direct_observation[id1] matches {	-- Nutrition intake
		data matches {
			allow_archetype Node[id11] matches {	-- Nutrients
				include
					archetype_id/value matches {/s2-EHR-Node\.dietary_nutrients(-[a-zA-Z0-9_]+)*\.v0|s2-EHR-Node\.dietary_nutrients(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.macronutrients(-[a-zA-Z0-9_]+)*\.v0|s2-EHR-Node\.macronutrients(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.micronutrients(-[a-zA-Z0-9_]+)*\.v0|s2-EHR-Node\.micronutrients(-[a-zA-Z0-9_]+)*\.v1/}
			}
			Node[id12] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id17] 
				}
			}
		}
		qualifiers matches {
			Node[id14] occurrences matches {0..1} matches {	-- Nutritional day definition
				value matches {
					Time_value[id21] 
				}
			}
			allow_archetype Node[id15] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
		data_series matches {
			Event[id16] matches {	-- Meal
				items matches {
					use_node Node[id11] /data[id11]	-- /data[Nutrients]
					use_node Node[id12] /data[id12]	-- /data[Comment]
				}
			}
			Interval_event[id5] matches {	-- Cumulative total
				items matches {
					use_node Node[id11] /data[id11]	-- /data[Nutrients]
					use_node Node[id12] /data[id12]	-- /data[Comment]
				}
				sample_function matches {"total"}
			}
		}
	}

