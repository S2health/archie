    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record the text conversation, or video- or audio-conference recording, between two or more clinicians, captured as part of the record of health care.">
            keywords = <"chat", "videoconference", "audioconference", "conversation", "discussion">
            use = <"Use to document evidence of interactions between healthcare providers that support the record of health care. For example, use to record phone or online chat conversations between remote healthcare providers; or audio- and video-conference recordings that record aspects of telemedicine consultations or case conferences.">
            misuse = <"">
        >
    >
    
definition
    Direct_observation[id1] matches {    -- Conference
        data matches {
            Node[id5] occurrences matches {0..1} matches {    -- Text record
                value matches {
                    Text[id11]
                }
            }
            Node[id6] occurrences matches {0..1} matches {    -- Audio or video record
                value matches {
                    Multimedia[id12] matches {
                        media_type matches {[ac1]}    -- Audio or video record (synthesised)
                    }
                }
            }
            Node[id8] occurrences matches {0..1} matches {    -- Author
                value matches {
                    Text[id13]
                }
            }
        }
        qualifiers matches {
            allow_archetype Node[id10] matches {     -- Extension
                include
                    archetype_id/value matches {/.*/}
            }
        }
        data_series matches {
            Interval_event[id7] matches {    -- Recording interval
                items matches {
                    use_node Node[id5] /data[id5]    -- Text record
                    use_node Node[id6] /data[id6]    -- Audio or video record
                    use_node Node[id8] /data[id8]    -- Author
                }
                sample_function matches {"actual"}
            }
        }
    }

