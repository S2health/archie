	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"Use to record the ratio between the partial pressure of oxygen in blood (PaO₂) and the fraction of oxygen in the inspired air (FiO₂).">
			use = <"Use to record the ratio between the partial pressure of oxygen in blood (PaO₂), and the fraction of oxygen in the inspired air (FiO₂).
            
            The value of PaO₂ will usually be recorded elsewhere in the health record as part of an arterial blood gas test result (OBSERVATION.laboratory_test_result).
            
            The value of FiO₂ will usually be recorded elsewhere in the health record using the 'FiO₂' data Node (at0052) in the Node.inspired_oxygen.
            
            This ratio may be used within other scores, such as SOFA Score (OBSERVATION.sofa_score), SMART-COP risk score, ARDS severity score and Simplified Acute Physiology Score (SAPS) II.">
			keywords = <"Carrico", "Horowitz", "index", "ratio", "PF", "P/F", "PaO2", "FiO2">
			misuse = <"">
		>
	>
