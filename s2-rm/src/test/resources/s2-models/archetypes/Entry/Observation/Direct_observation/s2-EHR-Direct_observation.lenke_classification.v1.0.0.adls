definition
    Direct_observation[id1] matches {    -- Lenke classification system
        data matches {
            Node[id5] occurrences matches {0..1} matches {    -- Curve type
                value matches {
                    Coded_text[id23] matches {
                        term matches {
                            Terminology_term[id27] matches {
                                concept matches {[ac1]}    -- Curve type (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id12] occurrences matches {0..1} matches {    -- Lumbar spine modifier
                value matches {
                    Coded_text[id24] matches {
                        term matches {
                            Terminology_term[id28] matches {
                                concept matches {[ac2]}    -- Lumbar spine modifier (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id16] occurrences matches {0..1} matches {    -- Thoracic sagittal modifier
                value matches {
                    Coded_text[id25] matches {
                        term matches {
                            Terminology_term[id29] matches {
                                concept matches {[ac3]}    -- Thoracic sagittal modifier (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id20] occurrences matches {0..1} matches {    -- Classification
                value matches {
                    Text[id26]
                }
            }
        }
        qualifiers matches {
            allow_archetype Node[id21] matches {     -- Extension
                include
                    archetype_id/value matches {/.*/}
            }
        }
    }

