    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record the result of a Karnofsky Performance Status (KPS) scale assessment.">
            keywords = <"KPS", "assessment", "functional", "performance", "oncology", "cancer", "Karnofsky", "status", "quality", "score", "index">
            use = <"Use to record the result of a Karnofsky Performance Status (KPS) scale assessment.
            
            While the original 1949 paper by Karnofsky et al is referenced in this archetype, it has not been possible to obtain a copy to verify the original content. However, this archetype has been designed to reflect the practical use of Karnofsky Performance Status scale rather than an academic construct and the current wording reflects descriptions found in common use within the other references.">
            misuse = <"Not to be used to record variations of the KPS scale, such as the Thorne-modified Karnofsky (TKPS) or the Australia-modified Karnofsky (AKPS). Use purpose-specific archetypes for this purpose.">
        >
    >
    
definition
    Score[id1] matches {    -- Karnofsky Performance Status (KPS) scale
        data matches {
            Node[id5] matches {    -- Karnofsky performance status
                value matches {
                    Coded_ordinal[id20] matches {
                        magnitude matches {100}
                        concept matches {
                            Coded_text[id22] matches {
                                term matches {
                                    Terminology_term[id21] matches {
                                        concept matches {[at15]}    -- Normal; no complaints; no evidence of disease
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id23] matches {
                        magnitude matches {90}
                        concept matches {
                            Coded_text[id25] matches {
                                term matches {
                                    Terminology_term[id24] matches {
                                        concept matches {[at14]}    -- Able to carry on normal activity; minor signs or symptoms of disease
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id26] matches {
                        magnitude matches {80}
                        concept matches {
                            Coded_text[id28] matches {
                                term matches {
                                    Terminology_term[id27] matches {
                                        concept matches {[at13]}    -- Normal activity with effort; some signs or symptoms of disease
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id29] matches {
                        magnitude matches {70}
                        concept matches {
                            Coded_text[id31] matches {
                                term matches {
                                    Terminology_term[id30] matches {
                                        concept matches {[at12]}    -- Cares for self; unable to carry on normal activity or to do active work
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id32] matches {
                        magnitude matches {60}
                        concept matches {
                            Coded_text[id34] matches {
                                term matches {
                                    Terminology_term[id33] matches {
                                        concept matches {[at11]}    -- Requires occasional assistance, but is able to care for most of his personal needs
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id35] matches {
                        magnitude matches {50}
                        concept matches {
                            Coded_text[id37] matches {
                                term matches {
                                    Terminology_term[id36] matches {
                                        concept matches {[at10]}    -- Requires considerable assistance and frequent medical care
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id38] matches {
                        magnitude matches {40}
                        concept matches {
                            Coded_text[id40] matches {
                                term matches {
                                    Terminology_term[id39] matches {
                                        concept matches {[at9]}    -- Disabled; requires special care and assistance
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id41] matches {
                        magnitude matches {30}
                        concept matches {
                            Coded_text[id43] matches {
                                term matches {
                                    Terminology_term[id42] matches {
                                        concept matches {[at8]}    -- Severely disabled; hospital admission is indicated although death not imminent
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id44] matches {
                        magnitude matches {20}
                        concept matches {
                            Coded_text[id46] matches {
                                term matches {
                                    Terminology_term[id45] matches {
                                        concept matches {[at7]}    -- Very sick; hospital admission necessary; active supportive treatment necessary
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id47] matches {
                        magnitude matches {10}
                        concept matches {
                            Coded_text[id49] matches {
                                term matches {
                                    Terminology_term[id48] matches {
                                        concept matches {[at6]}    -- Moribund; fatal processes progressing rapidly.
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id50] matches {
                        magnitude matches {0}
                        concept matches {
                            Coded_text[id52] matches {
                                term matches {
                                    Terminology_term[id51] matches {
                                        concept matches {[at5]}    -- Dead
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        qualifiers matches {
            allow_archetype Node[id18] matches {     -- Extension
                include
                    archetype_id/value matches {/.*/}
            }
        }
    }

