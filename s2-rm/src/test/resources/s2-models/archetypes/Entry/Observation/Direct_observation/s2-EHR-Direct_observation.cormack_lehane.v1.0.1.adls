    Direct_observation[id1] matches {    -- Cormack-Lehane classification
        data cardinality matches {1..*; unordered} matches {
            Node[id2] occurrences matches {0..1} matches {    -- Grading
                value matches {
                    Coded_text[id13] matches {
                        term matches {
                            Terminology_term[id15] matches {
                                concept matches {[ac1]}    -- Grading (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id7] occurrences matches {0..1} matches {    -- Comment
                value matches {
                    Text[id14]
                }
            }
        }
        qualifiers matches {
            allow_archetype Node[id12] matches {     -- Extension
                include
                    archetype_id/value matches {/.*/}
            }
        }
    }

