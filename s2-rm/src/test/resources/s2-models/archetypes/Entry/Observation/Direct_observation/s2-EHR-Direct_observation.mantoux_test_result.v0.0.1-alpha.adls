definition
	Direct_observation[id1] matches {	-- Mantoux test result
		data matches {
			Node[id12] occurrences matches {0..3} matches {	-- Diameter
				value matches {
					Measured<Quantity>[id24] matches {
						value matches {
							Quantity[id18] matches {
								magnitude matches {|>=0.0|}
								precision matches {0}
								units matches {
									Coded_text[id26] matches {
										term matches {
											Terminology_term[id25] matches {
												concept matches {[at12]}
											}
										}
									}
								}
							}
						}
					}
				}
			}
			Node[id13] occurrences matches {0..1} matches {	-- Clinical description
				value matches {
					Text[id19] 
				}
			}
			Node[id6] occurrences matches {0..1} matches {	-- Interpretation
				value matches {
					Coded_text[id20] matches {
						term matches {
							Terminology_term[id27] matches {
								concept matches {[ac1]}		-- Interpretation (synthesised)
							}
						}
					}
					Plain_text[id21] 
				}
			}
			use_archetype Node[id17, s2-EHR-Node.media_file.v1] 
			Node[id9] matches {	-- Comment
				value matches {
					Text[id22] 
				}
			}
		}
		qualifiers matches {
			Node[id8] matches {	-- Rationale for interpretation
				value matches {
					Text[id23] 
				}
			}
			allow_archetype Node[id16] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

