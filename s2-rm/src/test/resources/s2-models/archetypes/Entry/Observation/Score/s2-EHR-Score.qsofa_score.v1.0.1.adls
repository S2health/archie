                                term matches {
                                    Terminology_term[id26] matches {
                                        concept matches {[at9]}    -- Respiration rate <22
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id28] matches {
                        magnitude matches {1}
                        concept matches {
                            Coded_text[id30] matches {
                                term matches {
                                    Terminology_term[id29] matches {
                                        concept matches {[at10]}    -- Respiration rate ≥22
                                    }
                                }
                            }
                        }
                    }
                }
            }
            Node[id8] occurrences matches {0..1} matches {    -- Blood pressure
                value matches {
                    Coded_ordinal[id31] matches {
                        magnitude matches {0}
                        concept matches {
                            Coded_text[id33] matches {
                                term matches {
                                    Terminology_term[id32] matches {
                                        concept matches {[at14]}    -- Systolic blood pressure >100
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id34] matches {
                        magnitude matches {1}
                        concept matches {
                            Coded_text[id36] matches {
                                term matches {
                                    Terminology_term[id35] matches {
                                        concept matches {[at15]}    -- Systolic blood pressure ≤100
                                    }
                                }
                            }
                        }
                    }
                }
            }
            Node[id9] occurrences matches {0..1} matches {    -- Mental status
                value matches {
                    Coded_ordinal[id37] matches {
                        magnitude matches {0}
                        concept matches {
                            Coded_text[id39] matches {
                                term matches {
                                    Terminology_term[id38] matches {
                                        concept matches {[at11]}    -- No altered mental status
                                    }
                                }
                            }
                        }
                    }
                    Coded_ordinal[id40] matches {
                        magnitude matches {1}
                        concept matches {
                            Coded_text[id42] matches {
                                term matches {
                                    Terminology_term[id41] matches {
                                        concept matches {[at12]}    -- Altered mental status
                                    }
                                }
                            }
                        }
                    }
                }
            }
            Node[id6] occurrences matches {0..1} matches {    -- qSOFA score
                value matches {
                    Count[id23] matches {
                        magnitude matches {|0..3|}
                    }
                }
            }
            Node[id5] occurrences matches {0..1} matches {    -- Comment
                value matches {
                    Text[id24]
                }
            }
        }
        qualifiers matches {
            allow_archetype Node[id19] matches {     -- Extension
