    term_definitions = <
        ["en"] = <
            ["ac1"] = <
                text = <"Respiration (synthesised)">
                description = <"The ratio between the partial pressure of oxygen (PaO₂) and the fraction of inspired oxygen (FiO₂) is an indicator for a possible dysfunction of the respiratory system. (synthesised)">
            >
            ["ac2"] = <
                text = <"Cardiovascular system (synthesised)">
                description = <"The mean arterial pressure (MAP), or the need for vasopressors (VP), (dopamine (DA), adrenaline (A), noradrenaline (NA) or dobutamine) are indicators for a possible dysfunction of the cardiovascular system. (synthesised)">
            >
            ["ac3"] = <
                text = <"Central nervous system (synthesised)">
                description = <"The Glasgow Coma Scale (GCS) is an indicator for a possible dysfunction of the central nervous system. (synthesised)">
            >
            ["ac4"] = <
                text = <"Renal function (synthesised)">
                description = <"Creatinine concentration and 24 h urine output (UOP) are indicators for a possible dysfunction of the central nervous system. (synthesised)">
            >
            ["ac5"] = <
                text = <"Liver function (synthesised)">
                description = <"Bilirubin concentration is an indicator for a possible dysfunction of the central nervous system. (synthesised)">
            >
            ["ac6"] = <
                text = <"Blood clotting (synthesised)">
                description = <"Platelets concentration is an indicator for a possible dysfunction of the blood clotting system. (synthesised)">
            >
            ["at11"] = <
                text = <"MAP ≥ 70 mmHg">
                description = <"The mean arterial pressure (MAP) is greater than or equal to 70 mmHg.">
            >
            ["at12"] = <
                text = <"MAP < 70 mmHg">
                description = <"The mean arterial pressure (MAP) is less than 70 mmHg.">
            >
            ["at13"] = <
                text = <"DA ≤ 5; Dobutamine">
                description = <"The dopamine (DA) dosage is less than 5 µg/kg/minute, or any dosage of dobutamine.">
            >
            ["at14"] = <
                text = <"DA > 5; NA/A ≤ 0.1">
                description = <"The dosage of dopamine (DA) is greater than 5 µg/kg/minute, or the dosage of noradrenaline (NA) is less than or equal to 0.1 µg/kg/minute, or the dosage of adrenaline (A) is less than or equal to 0.1 µg/kg/minutt.">
            >
            ["at15"] = <
                text = <"NA/A > 0.1">
                description = <"The dosage of noradrenaline (N) or adrenaline (A) is greater than 0.1 µg/kg/minute.">
