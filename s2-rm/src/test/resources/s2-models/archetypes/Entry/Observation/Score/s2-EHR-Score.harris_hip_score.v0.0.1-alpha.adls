definition
    Score[id1] matches {    -- Harris hip score
        data matches {
            Node[id5] occurrences matches {0..1} matches {    -- Harris hip score result
                value matches {
                    Quantity[id13] matches {
                        magnitude matches {|0.0..100.0|}
                        precision matches {2}
                        units matches {
                            Coded_text[id16] matches {
                                term matches {
                                    Terminology_term[id15] matches {
                                        concept matches {[at11]}    -- 1
                                    }
                                }
                            }
                        }
                    }
                }
            }
            Node[id6] occurrences matches {0..1} matches {    -- Grading
                value matches {
                    Coded_text[id14] matches {
                        term matches {
                            Terminology_term[id17] matches {
                                concept matches {[ac1]}    -- Grading (synthesised)
                            }
                        }
                    }
                }
            }
        }
        qualifiers matches {
            allow_archetype Node[id12] matches {     -- Extension
                include
                    archetype_id/value matches {/.*/}
            }
        }
    }

