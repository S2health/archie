definition
	Order[id1] matches {	-- Transfusion order
		activities matches {
			Activity[id2] occurrences matches {0..1} matches {	-- new activity
				description cardinality matches {2..*; unordered} matches {
					Node[id4] matches {	-- Type of blood product
						value matches {
							Text[id17] 
						}
					}
					Node[id11] matches {	-- Amount
						items matches {
							Node[id5] occurrences matches {0..1} matches {	-- Number of units
								value matches {
									Count[id18] matches {
										magnitude matches {|>=0|}
									}
								}
							}
							Node[id12] occurrences matches {0..1} matches {	-- Volume
								value matches {
									Quantity[id19] matches {
										magnitude matches {|>=0.0|}
										precision matches {0}
										units matches {
											Coded_text[id24] matches {
												term matches {
													Terminology_term[id23] matches {
														concept matches {[at2]}
													}
												}
											}
										}
									}
								}
							}
						}
					}
					Node[id10] occurrences matches {0..1} matches {	-- Clinical indication
						value matches {
							Text[id20] 
						}
					}
					Node[id13] occurrences matches {0..1} matches {	-- Urgency
						value matches {
							Text[id21] 
						}
					}
					allow_archetype Node[id14] matches {	-- Administration pattern
						include
							archetype_id/value matches {/.*/}
					}
					Node[id6] matches {	-- Special requirement
						value matches {
							Text[id22] 
						}
					}
				}
			}
		}
		qualifiers matches {
			allow_archetype Node[id16] matches {	-- Extension
				include
					archetype_id/value matches {/.*/}
			}
		}
	}

