					}
				}
			}
			Info_node[id153] occurrences matches {0..1} matches {	-- Strength (formulation)
				value matches {
					Proportion[id163] 
				}
			}
			Info_node[id152] occurrences matches {0..1} matches {	-- Manufacturer
				value matches {
					Text[id166] 
				}
			}
		}
	}

