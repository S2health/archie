		>
	>

definition
	Info_node[id1] matches {	-- Structured address
		value matches {
			Plain_text[id39] 
		}
		items matches {
			Info_node[id2] matches {	-- Address line
				value matches {
					Plain_text[id23] 
				}
			}
			Info_node[id3] matches {	-- City/Town
				value matches {
					Plain_text[id24] 
				}
			}
			Info_node[id4] matches {	-- District/County
				value matches {
					Plain_text[id25] 
				}
			}
			Info_node[id5] occurrences matches {0..1} matches {	-- State/Territory/Province
				value matches {
					Plain_text[id26] 
				}
			}
			Info_node[id6] occurrences matches {0..1} matches {	-- Postal code
				value matches {
					Plain_text[id27] 
				}
			}
			Info_node[id7] occurrences matches {0..1} matches {	-- Country
				value matches {
					Coded_text[id39] matches {
						term matches {
							Terminology_term[id139] matches {
								concept matches {[ac7]}		-- Country
							}
						}
					}
				}
			}
			Info_node[id22] occurrences matches {0..1} matches {	-- Geolocation code
				value matches {
					Plain_text[id29] 
				}
			}
			Info_node[id8] occurrences matches {0..1} matches {	-- Latitude
				value matches {
					Quantity[id30] matches {
						magnitude matches {|-90.0..90.0|}
						precision matches {6}
						units matches {
							Coded_text[id40] matches {
								term matches {
									Terminology_term[id39] matches {
										concept matches {[at20]}
									}
								}
							}
						}
					}
				}
			}
			Info_node[id9] occurrences matches {0..1} matches {	-- Longitude
				value matches {
					Quantity[id31] matches {
						magnitude matches {|-180.0..180.0|}
						precision matches {6}
						units matches {
							Coded_text[id42] matches {
								term matches {
									Terminology_term[id41] matches {
										concept matches {[at20]}
									}
								}
							}
						}
					}
				}
			}
			Info_node[id10] occurrences matches {0..1} matches {	-- Altitude
				value matches {
					Quantity[id32] matches {
						precision matches {2}
						units matches {
							Coded_text[id44] matches {
								term matches {
									Terminology_term[id43] matches {
										concept matches {[at21]}
									}
								}
							}
						}
					}
				}
			}
			Info_node[id20] occurrences matches {0..1} matches {	-- Map URL
				value matches {
					Text[id33] 
				}
			}
		}
	}

