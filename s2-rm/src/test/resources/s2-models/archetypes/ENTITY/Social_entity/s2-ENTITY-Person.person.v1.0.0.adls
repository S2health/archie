definition
	Person[id1] matches {	-- Person
		comms_addresses matches {
			use_archetype Comms_address_use[id2, s2-ENTITY-Comms_address_use.comms_address_use_general.v1] 
		}
		location_addresses matches {
			use_archetype Location_address_use[id6, s2-ENTITY-Location_address_use.location_address_use_general.v1] 
		}
		identities matches {
			use_archetype Party_identity[id7, s2-ENTITY-Party_identity.person_structured_name.v1] 
		}
		other_details matches {
			Info_node[id8] occurrences matches {0..1} matches {	-- Date of birth
				value matches {
					Date_value[id108] 
				}
			}
			Info_node[id9] occurrences matches {0..1} matches {	-- Place of birth
				value matches {
					Plain_text[id109] 
				}
			}
			Info_node[id12] occurrences matches {0..1} matches {	-- Sex
				value matches {
					Coded_text[id102] matches {
						term matches {
							Terminology_term[id1102] matches {
								concept matches {[ac2]}		-- Biological sex
							}
						}
					}
				}
			}
			Info_node[id13] occurrences matches {0..1} matches {	-- Date of decease
				value matches {
					Date_value[id103] 
				}
			}
			Info_node[id14] occurrences matches {0..1} matches {	-- Deceased
				value matches {
					Boolean_value[id104] 
				}
			}
			Info_node[id15] occurrences matches {0..1} matches {	-- Administrative gender
				value matches {
					Coded_text[id115] matches {
						term matches {
							Terminology_term[id1105] matches {
								concept matches {[ac5]}		-- Adminstrative gender
							}
						}
					}
				}
			}
			Info_node[id16] occurrences matches {0..1} matches {	-- Marital status
				value matches {
					Coded_text[id116] matches {
						term matches {
							Terminology_term[id1106] matches {
								concept matches {[ac6]}		-- Marital status
							}
						}
					}
				}
			}
			Info_node[id10] occurrences matches {0..1} matches {	-- Photo
				value matches {
					Multimedia[id110] 
				}
			}
			Info_node[id11] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Plain_text[id111] 
				}
			}
		}
	}

