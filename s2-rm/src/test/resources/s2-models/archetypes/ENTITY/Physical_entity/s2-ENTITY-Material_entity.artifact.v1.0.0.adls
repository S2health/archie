definition
	Material_entity[id1] matches {	-- Medical device
		kind matches {
			use_archetype Artifact_kind[id2, s2-ENTITY-Artifact_kind.device_general.v1]
		}
		individual_attributes matches {
			Info_node[id22] occurrences matches {0..1} matches {	-- Unique device identifier (UDI)
				value matches {
					Rwe_id_ref[id32] 
				}
			}
			Info_node[id6] occurrences matches {0..1} matches {	-- Date of manufacture
				value matches {
					Date_time_value[id34] 
				}
			}
			Info_node[id21] occurrences matches {0..1} matches {	-- Serial number
				value matches {
					Text[id35] 
				}
			}
			Info_node[id26] occurrences matches {0..1} matches {	-- Software version
				value matches {
					Text[id39] 
				}
			}
			Info_node[id8] occurrences matches {0..1} matches {	-- Date of expiry
				value matches {
					Date_time_value[id40] 
				}
			}
			Info_node[id25] matches {	-- Other identifier
				value matches {
					Rwe_id_ref[id41] 
				}
			}
		}
		lot_information matches {
			Info_node[id7] occurrences matches {0..1} matches {	-- Batch/Lot number
				value matches {
					Text[id38] 
				}
			}
		}
		use_details matches {
			Info_node[id9] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id42] 
				}
			}
		}
	}

