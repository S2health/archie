definition
	Artifact_kind[id1] matches {	-- General device description
		items matches {
			Info_node[id4] occurrences matches {0..1} matches {	-- Type
				value matches {
					Text[id30] 
				}
			}
			Info_node[id3] occurrences matches {0..1} matches {	-- Description
				value matches {
					Text[id31] 
				}
			}
			allow_archetype Info_node[id10] matches {	-- Properties
				include
					archetype_id/value matches {/.*/}
			}
			Info_node[id5] occurrences matches {0..1} matches {	-- Manufacturer
				value matches {
					Text[id33] 
				}
			}
			Info_node[id23] occurrences matches {0..1} matches {	-- Catalogue number
				value matches {
					Text[id36] 
				}
			}
			Info_node[id24] occurrences matches {0..1} matches {	-- Model number
				value matches {
					Text[id37] 
				}
			}
			Info_node[id26] occurrences matches {0..1} matches {	-- Software version
				value matches {
					Text[id39] 
				}
			}
			Info_node[id16] occurrences matches {0..1} matches {	-- Regulatory status
				value matches {
					Text[id48] 
				}
			}
			use_archetype Info_node[id28, s2-EHR-Info_node.media_file.v2] 
			Info_node[id9] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id42] 
				}
			}
		}
	}

