definition
	Node[id1] matches {	-- Interpreter request
		items matches {
			Node[id10] occurrences matches {0..1} matches {	-- Communication channel
				value matches {
					Coded_text[id41] matches {
						term matches {
							Terminology_term[id47] matches {
								concept matches {[ac1]}		-- Communication channel (synthesised)
							}
						}
					}
					Plain_text[id42] 
				}
			}
			Node[id40] occurrences matches {1..*} matches {	-- Per language
				name matches {"Preferred language", "Alternative language"}
				items matches {
					use_archetype Node[id32, s2-EHR-Node.language.v1] 
					Node[id36] occurrences matches {0..1} matches {	-- Comment
						value matches {
							Text[id44] 
						}
					}
				}
			}
			Node[id28] occurrences matches {0..1} matches {	-- Preferred gender
				value matches {
					Coded_text[id45] matches {
						term matches {
							Terminology_term[id48] matches {
								concept matches {[ac3]}		-- Preferred gender (synthesised)
							}
						}
					}
				}
			}
			allow_archetype Node[id5] matches {	-- Preferred interpreter
				include
					archetype_id/value matches {/s2-EHR-Node\.organization(-[a-zA-Z0-9_]+)*\.v0|s2-EHR-Node\.person_name(-[a-zA-Z0-9_]+)*\.v0|s2-EHR-Node\.individual_professional(-[a-zA-Z0-9_]+)*\.v0/}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id46] 
				}
			}
		}
	}

