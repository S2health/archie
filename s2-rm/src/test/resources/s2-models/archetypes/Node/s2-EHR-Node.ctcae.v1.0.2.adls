definition
    Node[id1] matches {    -- Common Terminology Criteria for Adverse Events (CTCAE)
        items matches {
            Node[id2] occurrences matches {0..1} matches {    -- Category
                value matches {
                    Text[id22]
                }
            }
            Node[id3] matches {    -- Term
                value matches {
                    Text[id23]
                }
            }
            Node[id4] matches {    -- Grade category
                value matches {
                    Coded_ordinal[id27] matches {
                        magnitude matches {0}
                        concept matches {
                            Coded_text[id29] matches {
                                term matches {
                                    Terminology_term[id28] matches {
                                        concept matches {[at21]}    -- Grade 0
                                    }
                                }
                            }
                        }
                    }
