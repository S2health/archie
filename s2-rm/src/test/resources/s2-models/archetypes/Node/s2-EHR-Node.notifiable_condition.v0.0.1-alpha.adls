definition
	Node[id1] matches {	-- Notifiable condition
		items matches {
			Node[id2] occurrences matches {0..1} matches {	-- Specimen surce
				value matches {
					Text[id9] 
				}
			}
			Node[id3] occurrences matches {0..1} matches {	-- Status
				value matches {
					Coded_text[id10] matches {
						term matches {
							Terminology_term[id14] matches {
								concept matches {[ac1]}		-- Status (synthesised)
							}
						}
					}
				}
			}
			Node[id6] occurrences matches {0..1} matches {	-- Date of notification
				value matches {
					Date_time_value[id11] 
				}
			}
			Node[id7] occurrences matches {0..1} matches {	-- Notifiable condition
				value matches {
					Text[id12] 
				}
			}
			Node[id8] occurrences matches {0..1} matches {	-- Record of notifiable condition
				value matches {
					Ehr_uri_ref[id13] 
				}
			}
		}
	}

