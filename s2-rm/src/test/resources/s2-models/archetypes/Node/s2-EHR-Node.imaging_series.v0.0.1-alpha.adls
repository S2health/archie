definition
    Node[id1] matches {    -- Imaging series
        items matches {
            Node[id17] occurrences matches {0..1} matches {    -- Series instance identifier
                value matches {
                    Rwe_id_ref[id24]
                }
            }
            Node[id18] occurrences matches {0..1} matches {    -- Series number
                value matches {
                    Count[id25]
                }
            }
            Node[id23] occurrences matches {0..1} matches {    -- Series description
                value matches {
                    Text[id26]
                }
            }
            Node[id19] occurrences matches {0..1} matches {    -- Modality
                value matches {
                    Text[id27]
                }
            }
            Node[id11] occurrences matches {0..1} matches {    -- Number of instances
                value matches {
                    Count[id28]
                }
            }
            Node[id20] matches {    -- Series end point
                value matches {
                    Uri_ref[id29]
                    Text[id30]
                }
            }
            allow_archetype Node[id22] matches {     -- Device
                include
                    archetype_id/value matches {/.*/}
            }
        }
    }

terminology
