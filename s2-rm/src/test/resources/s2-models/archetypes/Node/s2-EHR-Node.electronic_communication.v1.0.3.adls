						term matches {
							Terminology_term[id22] matches {
								concept matches {[ac1]}		-- Type (synthesised)
							}
						}
					}
				}
			}
			Node[id3] matches {	-- Value
				value matches {
					Plain_text[id17] 
					Uri_ref[id18] 
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Purpose
				value matches {
					Plain_text[id19] 
					Coded_text[id20] matches {
						term matches {
							Terminology_term[id23] matches {
								concept matches {[ac2]}		-- Purpose (synthesised)
							}
						}
					}
				}
			}
			use_archetype Node[id14, s2-EHR-Node.language.v1] 
			Node[id5] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Plain_text[id21] 
