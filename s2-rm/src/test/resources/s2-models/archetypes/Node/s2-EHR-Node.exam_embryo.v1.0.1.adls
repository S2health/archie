                }
            }
            Node[id39] occurrences matches {0..1} matches {    -- Nucleation
                value matches {
                    Coded_text[id63] matches {
                        term matches {
                            Terminology_term[id73] matches {
                                concept matches {[ac3]}    -- Nucleation (synthesised)
                            }
                        }
                    }
                    Plain_text[id64]
                }
            }
            Node[id43] occurrences matches {0..1} matches {    -- Cytoplasmic morphology
                value matches {
                    Text[id65]
                }
