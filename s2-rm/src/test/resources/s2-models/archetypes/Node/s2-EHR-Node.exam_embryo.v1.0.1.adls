    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record structured observations about a human cleavage-stage embryo.">
            keywords = <"in vitro", "fertilization", "IVF", "reproductive medicine", "zygote", "fertilised", "fertilized", "oocyte", "egg", "fertilisation", "embryo", "reproduction", "ART", "blastocyst">
            use = <"Use to record morphological observations appropriate for the cleavage-stage of human embryo development, usually as part of assisted reproduction treatment. 
            
            This archetype has been specifically designed to be used in the 'Observations detail' SLOT within the 'OBSERVATION.embryo_assessment' or 'ACTION.procedure' archetype, but can also be used within other ENTRY or Node archetypes, where clinically appropriate.">
            misuse = <"Not to be used for recording observations on oocytes, zygotes and blastocysts - use specific Node archetypes for this purpose.">
        >
    >
    
definition
    Node[id1] matches {    -- Examination of a cleavage-stage embryo
        items matches {
            Node[id29] occurrences matches {0..1} matches {    -- Number of cells
                value matches {
                    Count[id57]
                }
            }
            Node[id31] occurrences matches {0..1} matches {    -- Fragmentation
                value matches {
                    Coded_text[id58] matches {
                        term matches {
                            Terminology_term[id71] matches {
                                concept matches {[ac1]}    -- Fragmentation (synthesised)
                            }
                        }
                    }
                    Plain_text[id59]
                    Ratio[id60]
                }
            }
            Node[id36] occurrences matches {0..1} matches {    -- Blastomere size
                value matches {
                    Coded_text[id61] matches {
                        term matches {
                            Terminology_term[id72] matches {
                                concept matches {[ac2]}    -- Blastomere size (synthesised)
                            }
                        }
                    }
