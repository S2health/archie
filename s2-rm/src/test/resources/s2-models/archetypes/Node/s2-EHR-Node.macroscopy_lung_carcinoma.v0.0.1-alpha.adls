	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"To record detailed findings about lung cancer found on macroscopic histopathological examination.">
			use = <"To record detailed findings about macroscopic examination of tissue related to lung cancer.
            
            This archetype has been designed to be used within the 'Structured findings' SLOT in the Node.histopathology_findings archetype.">
			keywords = <"histopathology", "cancer", "laboratory", "lab", "malignancy", "pathology", "lung", "bronchial">
			misuse = <"">
		>
	>

definition
	Node[id1] matches {	-- Macroscopic findings - Lung cancer
		items matches {
			Node[id3] occurrences matches {0..1} matches {	-- Number of tumours
				value matches {
					Count[id9] matches {
						magnitude matches {|>=0|}
					}
				}
			}
			use_archetype Node[id4, s2-EHR-Node.anatomical_location.v1] 
			use_archetype Node[id2, s2-EHR-Node.physical_properties.v0] 
			use_archetype Node[id5, s2-EHR-Node.tumour_invasion.v0] 
			use_archetype Node[id6, s2-EHR-Node.tumour_resection_margins.v0] 
			use_archetype Node[id7, s2-EHR-Node.lymph_node_metastases.v0] 
			Node[id8] occurrences matches {0..1} matches {	-- Non-neoplastic lung findings
				value matches {
					Text[id10] 
				}
			}
		}
	}

