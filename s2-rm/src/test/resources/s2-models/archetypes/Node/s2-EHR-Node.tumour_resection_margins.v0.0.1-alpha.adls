	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"Details of surgical resection margin involvement of malignant tumours. May be used to describe scenarios where tumour is present at the resection margin, detailing the length of tumour involvement, or where tumour is absent from the margin, to indicate the distance of tumour from that margin.">
			use = <"Normally used within a histopathology archetype.">
			keywords = <"malignancy", "margins", "tumour", "cancer", "resection", "histopathology", "biopsy">
			misuse = <"">
		>
	>
	other_contributors = <"Sabine Leh, Helse Bergen HF, Norway", "John Tore Valand, Helse Bergen HF, Norway", "Silje Ljosland Bakke, Nasjonal IKT HF, Norway", "Therese Högberg Mårder, Linköpings University, Sweden">

definition
	Node[id1] matches {	-- Surgical resection margins
		items matches {
			Node[id19] occurrences matches {0..1} matches {	-- Margin name
				value matches {
					Text[id23] 
				}
			}
			allow_archetype Node[id20] matches {	-- Margin location
				include
					archetype_id/value matches {/s2-EHR-Node\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v0/}
			}
			Node[id2] occurrences matches {0..1} matches {	-- Marginal involvement
				value matches {
					Coded_text[id24] matches {
						term matches {
							Terminology_term[id30] matches {
								concept matches {[ac1]}		-- Marginal involvement (synthesised)
							}
						}
					}
				}
			}
			Node[id7] matches {	-- Tumour present
				items matches {
					Node[id8] occurrences matches {0..1} matches {	-- Maximum linear involvement
						value matches {
							Quantity[id25] matches {
								magnitude matches {|>=0.0|}
								units matches {
									Coded_text[id32] matches {
										term matches {
											Terminology_term[id31] matches {
												concept matches {[at19]}
											}
										}
									}
								}
							}
						}
					}
					Node[id21] occurrences matches {0..1} matches {	-- Nature of involvement
						value matches {
							Text[id26] 
						}
					}
				}
			}
			Node[id9] matches {	-- Tumour absent
				items matches {
					Node[id10] occurrences matches {0..1} matches {	-- Distance from resection margin
						value matches {
							Quantity[id27] matches {
								magnitude matches {|>=0.0|}
								units matches {
									Coded_text[id34] matches {
										term matches {
											Terminology_term[id33] matches {
												concept matches {[at19]}
											}
										}
									}
								}
							}
						}
					}
					Node[id22] occurrences matches {0..1} matches {	-- Tumour name
						value matches {
