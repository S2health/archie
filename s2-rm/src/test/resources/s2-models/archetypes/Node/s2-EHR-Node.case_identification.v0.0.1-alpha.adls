definition
	Node[id1] matches {	-- Case identification
		items matches {
			Node[id2] occurrences matches {0..1} matches {	-- Case identifier
				value matches {
					Text[id8] 
				}
			}
			Node[id3] occurrences matches {0..1} matches {	-- Case started
				value matches {
					Date_time_value[id9] 
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Status
				value matches {
					Coded_text[id10] matches {
						term matches {
							Terminology_term[id12] matches {
								concept matches {[ac1]}		-- Status (synthesised)
							}
						}
					}
				}
			}
			Node[id7] occurrences matches {0..1} matches {	-- Case identified
				value matches {
					Text[id11] 
				}
			}
		}
	}

