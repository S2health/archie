    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record the details about a copy number variant of human DNA, observed in a genomic sequence.">
            keywords = <"copy number", "variation", "genetic", "genomic", "variant", "DNA", "chromosome", "mutation", "nucleotide">
            use = <"Use to record the details about a copy number variant of human DNA, observed in a genomic sequence.
            
            This archetype has been specifically designed to be used in the 'Structured variant' SLOT within the Node.genomic_variant_result archetype, but can also be used within other ENTRY or Node archetypes, where clinically appropriate.
            
            All definitions and examples in this archetype follow the HGVS nomenclature.">
            misuse = <"Not to be used to record information about variants of non-human DNA, or any kind of RNA or protein.
            
            Not to be used to record information about tandem repeats. Use the Node.genomic_repeated_sequence_variant archetype for this purpose.">
        >
    >
    
definition
    Node[id1] matches {    -- Genomic copy number variant
        items matches {
            Node[id2] occurrences matches {0..1} matches {    -- Start
                value matches {
                    Count[id7]
                    Range<Count>[id8]
                }
            }
            Node[id3] occurrences matches {0..1} matches {    -- End
                value matches {
                    Count[id9]
                    Range<Count>[id10]
                }
            }
            Node[id4] occurrences matches {0..1} matches {    -- Total copy number
                value matches {
                    Count[id11] matches {
                        magnitude matches {|>=0|}
                    }
                    Quantity[id12] matches {
                        units matches {
                            Coded_text[id15] matches {
                                term matches {
                                    Terminology_term[id14] matches {
                                        concept matches {[at9]}    -- 1
                                    }
                                }
                            }
                        }
                    }
                }
            }
            Node[id6] occurrences matches {0..1} matches {    -- Copy number change type
                value matches {
                    Coded_text[id13] matches {
                        term matches {
                            Terminology_term[id16] matches {
                                concept matches {[ac1]}    -- Copy number change type (synthesised)
                            }
                        }
                    }
                }
            }
            use_archetype Node[id5, s2-EHR-Node.reference_sequence.v1]    -- Reference sequence
        }
    }

