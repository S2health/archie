    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record highly detailed anatomical location.">
            keywords = <"coordinates", "specimen", "DICOM">
            use = <"Use where highly precise localisation information is required, using co-ordinates. This archetype has been designed to support DICOM based coordinate systems for accurate object localisation.
            
            Designed to fit within appropriate ENTRY or Node archetypes, especially Node.anatomical_location.">
            misuse = <"Not designed to be used as a standalone archetype.">
        >
    >
    
definition
    Node[id1] matches {    -- Precise anatomical location
        items matches {
            Node[id2] occurrences matches {0..1} matches {    -- Position frame of reference
                value matches {
                    Text[id6]
                }
            }
            Node[id3] occurrences matches {0..1} matches {    -- X offset
                value matches {
                    Quantity[id7]
                    Count[id8] matches {
                        magnitude matches {|>=0|}
                    }
                }
            }
            Node[id4] occurrences matches {0..1} matches {    -- Y offset
                value matches {
                    Quantity[id9]
                    Count[id10] matches {
                        magnitude matches {|>=0|}
                    }
                }
            }
            Node[id5] occurrences matches {0..1} matches {    -- Z offset
                value matches {
                    Quantity[id11]
                    Count[id12] matches {
                        magnitude matches {|>=0|}
                    }
                }
            }
        }
    }

