	details = <
		["en"] = <
			language = <[iso_639-1::en]>
			purpose = <"To record information about recognised subcategories of delay or specific delays in seeking, reaching or receiving healthcare using the \\\"Three Delays\\\" framework.">
			use = <"Use to record information about recognised subcategories of delay or specific delays in seeking, reaching or receiving healthcare using the \\\"Three Delays\\\" framework.
            
            This archetype has been designed to be nested within the 'Delay details' SLOT in the  ADMIN_ENTRY.three_delays_model archetype, so that variable levels of detail can be recorded.">
			misuse = <"">
		>
	>

definition
	Node[id1] matches {	-- Delay details
		items matches {
			Node[id2] occurrences matches {0..1} matches {	-- Delay name
				value matches {
					Text[id13] 
				}
			}
			Node[id3] occurrences matches {0..1} matches {	-- Delay presence?
				value matches {
					Coded_text[id14] matches {
						term matches {
							Terminology_term[id20] matches {
								concept matches {[ac1]}		-- Delay presence? (synthesised)
							}
						}
					}
				}
			}
			Node[id7] occurrences matches {0..1} matches {	-- Description
				value matches {
					Text[id15] 
				}
			}
			Node[id8] occurrences matches {0..1} matches {	-- Delay duration
				value matches {
					Duration_value[id16] 
				}
			}
			use_archetype Node[id11, s2-EHR-Node.delay_details.v0] 
			use_archetype Node[id10, s2-EHR-Node.person.v1] 
			Node[id9] occurrences matches {0..1} matches {	-- Solution timeline
				value matches {
					Date_time_value[id17] 
					Duration_value[id18] matches {
						magnitude matches {|>=PT0S|}
					}
				}
			}
			Node[id12] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id19] 
				}
			}
		}
	}

