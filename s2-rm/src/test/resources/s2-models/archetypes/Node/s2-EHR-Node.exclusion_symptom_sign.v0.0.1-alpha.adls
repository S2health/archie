        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To record a positive statement to explicitly record that a symptom or sign was reported as not present.">
            keywords = <"symptom", "absence", "exclusion">
            use = <"Use to record a positive statement to explicitly record that a symptom or sign was reported as not present.
            Use to record exlicitly that a symptom or sign was absolutely not present. If this is not absolutely required, consider use of the 'Nil significant' data Node within the Node.symptom_sign archetype.
            
            This archetype has been specifically designed to be used in the 'Structured detail' SLOT within the OBSERVATION.story archetype, but can also be used within other OBSERVATION or Node archetypes, where clinically appropriate.">
