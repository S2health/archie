definition
	Node[id1] matches {	-- Sequencing assay
		items matches {
			Node[id2] occurrences matches {0..1} matches {	-- Sequencing technology
				value matches {
					Coded_text[id39] matches {
						term matches {
							Terminology_term[id53] matches {
								concept matches {[ac1]}		-- Sequencing technology (synthesised)
							}
						}
					}
					Plain_text[id40] 
				}
			}
			use_archetype Node[id37, s2-EHR-Node.device.v1] 
			Node[id3] occurrences matches {0..1} matches {	-- Kit name
				value matches {
					Plain_text[id41] 
					Coded_text[id42] matches {
						term matches {
							Terminology_term[id54] matches {
								concept matches {[at21]}
							}
						}
					}
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Nucleic acid
				value matches {
					Plain_text[id43] 
					Coded_text[id44] matches {
						term matches {
							Terminology_term[id55] matches {
								concept matches {[ac2]}		-- Nucleic acid (synthesised)
							}
						}
					}
				}
			}
			Node[id21] occurrences matches {0..1} matches {	-- Tumor cell percentage
				value matches {
					Count[id45] matches {
						magnitude matches {|0..100|}
					}
				}
			}
			Node[id7] matches {	-- Tested Genes
				items matches {
					Node[id8] matches {	-- Gen symbol
						value matches {
							Text[id46] 
						}
					}
					Node[id9] occurrences matches {0..1} matches {	-- Gen name
						value matches {
							Text[id47] 
						}
					}
					use_archetype Node[id26, s2-EHR-Node.reference_sequence.v1] 
				}
			}
			Node[id31] matches {	-- Tested Region
				items matches {
					Node[id32] occurrences matches {0..1} matches {	-- Chromosomal location
						value matches {
							Plain_text[id48] 
							Coded_text[id49] 
						}
					}
					Node[id33] occurrences matches {0..1} matches {	-- Start
						value matches {
							Count[id50] 
						}
					}
					Node[id34] occurrences matches {0..1} matches {	-- End
						value matches {
							Count[id51] 
						}
					}
					use_archetype Node[id36, s2-EHR-Node.reference_sequence.v1] 
				}
			}
			allow_archetype Node[id27] matches {	-- Extensions
				include
					archetype_id/value matches {/.*/}
			}
			Node[id38] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id52] 
				}
			}
		}
	}

