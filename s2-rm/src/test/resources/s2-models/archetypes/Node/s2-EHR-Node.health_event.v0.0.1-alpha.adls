definition
	Node[id1] matches {	-- Health event
		items matches {
			Node[id2] matches {	-- Event name
				value matches {
					Text[id12] 
				}
			}
			Node[id3] occurrences matches {0..1} matches {	-- Description
				value matches {
					Text[id13] 
				}
			}
			allow_archetype Node[id4] matches {	-- Preceding symptoms and events
				include
					archetype_id/value matches {/s2-EHR-Node\.health_event(-[a-zA-Z0-9_]+)*\.v0|s2-EHR-Node\.symptom_sign(-[a-zA-Z0-9_]+)*\.v1/}
			}
			allow_archetype Node[id5] matches {	-- Associated symptoms and events
				include
					archetype_id/value matches {/s2-EHR-Node\.symptom_sign(-[a-zA-Z0-9_]+)*\.v1|s2-EHR-Node\.health_event(-[a-zA-Z0-9_]+)*\.v0/}
			}
			Node[id6] occurrences matches {0..1} matches {	-- Time elapsed
				value matches {
					Duration_value[id14] 
				}
			}
			use_archetype Node[id7, s2-EHR-Node.person.v1] 
			Node[id8] occurrences matches {0..1} matches {	-- Circumstances
				value matches {
					Text[id15] 
				}
			}
			Node[id9] matches {	-- Contributing factor
				value matches {
					Text[id16] 
				}
			}
			allow_archetype Node[id10] matches {	-- Additional details
				include
					archetype_id/value matches {/.*/}
			}
			Node[id11] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id17] 
				}
			}
		}
	}

