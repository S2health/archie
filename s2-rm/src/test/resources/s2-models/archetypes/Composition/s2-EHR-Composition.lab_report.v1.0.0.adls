definition
	Composition[id1] matches {	-- Lab report
		code matches {
			Terminology_term[id10] matches {
				concept matches {[at1]}
			}
		}
		category matches {
			Terminology_term[id11] matches {
				concept matches {[at2]}
				description matches {"persistent"}
			}
		}
		context matches {
			Event_context[id2] occurrences matches {1} matches {
				health_care_facility existence matches {1} 
				other_context matches {
					Node[id3] occurrences matches {1} matches {	-- Report identifier
						value matches {
							Rwe_id_ref[id8] 
						}
					}
					Node[id9] occurrences matches {1} matches {	-- Report status
						value matches {
							Text[id10] 
						}
					}
				}
			}
		}
	}

