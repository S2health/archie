definition
    Person[id1] matches {    -- Person
        description matches {
            allow_archetype Node[id2] occurrences matches {1} matches {     -- Demographic details
                include
                    archetype_id/value matches {/s2-Entity-Node\.person_details[a-zA-Z0-9_-]*\.v0/}
            }
        }
        identities matches {
            use_archetype Party_identity[id3, s2-Entity-Party_identity.person_name.v0] occurrences matches {1}     -- Name
        }
        contacts matches {
            Contact[id4] occurrences matches {1} matches {    -- Contacts
                addresses matches {
                    allow_archetype Address[id31] occurrences matches {1} matches {     -- Addresses
                        include
                            archetype_id/value matches {/s2-Entity-Address\.address([a-zA-Z0-9_-]+)*\.v0/}
                            archetype_id/value matches {/s2-Entity-Address\.electronic_communication[a-zA-Z0-9_-]*\.v0/}
                    }
                }
            }
        }
        relationships matches {
            Party_relationship[id5] matches {    -- Relationships
                details matches {
                    Node[id41] matches {    -- Relationship type
                        value matches {
                            Plain_text[id43]
                            Coded_text[id44] matches {
                                term matches {
                                    Terminology_term[id45] matches {
                                        concept matches {[ac1]}    -- Codes for type of relationship
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

