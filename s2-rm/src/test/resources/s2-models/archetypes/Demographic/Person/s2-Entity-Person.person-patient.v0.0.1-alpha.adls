        /relationships matches {
            Party_relationship[id0.2] matches {    -- Third party payer
                details matches {
                    use_archetype Node[id0.21, s2-Entity-Node.person_identifier.v0]    -- Healthcare consumer identifier.
                }
            }
            Party_relationship[id0.3] matches {    -- Healthcare provider/Health professional
                details matches {
                    use_archetype Node[id0.31, s2-Entity-Node.person_identifier.v0]    -- Patient identifier
                }
            }
        }
        /relationships[id5.1]/description matches {
