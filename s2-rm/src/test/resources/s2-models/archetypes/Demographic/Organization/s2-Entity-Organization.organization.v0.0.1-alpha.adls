definition
    Organization[id1] matches {    -- Organization
        description cardinality matches {1..*; unordered} matches {
            use_archetype Node[id11, s2-Entity-Node.provider_identifier.v0]    -- Identifier
        }
        identities matches {
            use_archetype Party_identity[id3, s2-Entity-Party_identity.organization_name.v0] occurrences matches {1..*}     -- Name
        }
        contacts matches {
            Contact[id4] matches {    -- Contacts
                addresses matches {
                    allow_archetype Address[id31] matches {     -- Address
                        include
                            archetype_id/value matches {/s2-Entity-Address\.address[a-zA-Z0-9_-]*\.v0/}
                            archetype_id/value matches {/s2-Entity-Address\.electronic_communication[a-zA-Z0-9_-]*\.v0/}
                    }
                }
            }
        }
        relationships matches {
            Party_relationship[id5] matches {    -- Relationship
                details cardinality matches {1..*; unordered} matches {
                    Node[id42] occurrences matches {0..1} matches {    -- Type of relationship
                        value matches {
                            Plain_text[id43]
                            Coded_text[id44] matches {
                                term matches {
                                    Terminology_term[id45] matches {
                                        concept matches {[ac1]}    -- Codes for type of relationship
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }

