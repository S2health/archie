terminology
	term_definitions = <
		["en"] = <
			["ac1"] = <
				text = <"Organization name (synthesised)">
				description = <"An organization name. (synthesised)">
			>
			["at20"] = <
				text = <"Organizational unit/section/division name">
				description = <"Used where a business unit, section, or division within an organization mat have its own separate identity.">
			>
			["at21"] = <
				text = <"Service location name">
				description = <"Used where the service location name is an important part of the organization name and is used for identification purposes, e.g. Mobile immunization Unit at Bankstown.">
			>
			["at22"] = <
				text = <"Business name">
				description = <"Business name used only for trading purposes.">
			>
			["at23"] = <
				text = <"Locally used name">
				description = <"Used for local names, e.g. where a medical practice is known by a name that is different from the company registration name or business name.">
			>
			["at24"] = <
				text = <"Abbreviated name">
				description = <"A short name or an abbreviated name by which the organization is known, e.g., HIC.">
			>
			["at25"] = <
				text = <"Enterprise name">
				description = <"Generally the complete organization name should be used to avoid any ambiguity in identification. This should usually be the same as the company registration name.">
			>
			["at26"] = <
				text = <"Other">
				description = <"Used when the organization name does not fit into any one of the categories listed above.">
			>
			["at27"] = <
				text = <"Unknown">
				description = <"Used when the organization name type is unknown.">
			>
			["id1"] = <
				text = <"Organization name">
				description = <"An organization name.">
			>
			["id11"] = <
				text = <"Name">
				description = <"The name by which an organization is known or called.">
			>
			["id12"] = <
				text = <"Identifier">
				description = <"The unique identifier of this name for this organization.">
			>
		>
	>
