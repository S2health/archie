definition
    Persona[id1] matches {    -- Individual healthcare provider
        identities matches {
            use_archetype Party_identity[id2, s2-Entity-Party_identity.person_name.v0] occurrences matches {1}     -- Name
        }
        capabilities matches {
            use_archetype Capability[id3, s2-Entity-Capability.individual_credentials.v0]    -- Capability
        }
        contacts matches {
            Contact[id4] matches {    -- Contact
                addresses matches {
                    allow_archetype Address[id31] occurrences matches {1} matches {     -- Address
                        include
                            archetype_id/value matches {/s2-Entity-Address\.address([a-zA-Z0-9_]+)*\.v0/}
                            archetype_id/value matches {/s2-Entity-Address\.electronic_communication[a-zA-Z0-9_-]*\.v0/}
                    }
                }
            }
        }
        relationships matches {
            Party_relationship[id5] matches {    -- Relationship with a third-party payer
                details matches {
                    use_archetype Node[id42, s2-Entity-Node.provider_identifier.v0] occurrences matches {1}     -- Identifier
                }
            }
            Party_relationship[id6] matches {    -- Employer/employee relationship
                details cardinality matches {1..*; unordered} matches {
                    use_archetype Node[id52, s2-Entity-Node.provider_identifier.v0] occurrences matches {1}     -- Identifier
                    Node[id53] occurrences matches {0..1} matches {    -- Employment role
                        value matches {
                            Text[id54]
                        }
                    }
                }
            }
        }
    }

