definition
    Persona[id1] matches {    -- Healthcare consumer
        identities matches {
            use_archetype Party_identity[id2, s2-Entity-Party_identity.person_name.v0]    -- Consumer name
        }
        relationships matches {
            Party_relationship[id3] matches {    -- Consumer-Third party payer
                details cardinality matches {1..*; unordered} matches {
                    use_archetype Node[id22, s2-Entity-Node.person_identifier.v0]    -- Healthcare consumer identifier
                }
            }
            Party_relationship[id4] matches {    -- Patient
                details cardinality matches {1..*; unordered} matches {
                    use_archetype Node[id32, s2-Entity-Node.person_identifier.v0]    -- Patient identifier
                }
            }
        }
    }

