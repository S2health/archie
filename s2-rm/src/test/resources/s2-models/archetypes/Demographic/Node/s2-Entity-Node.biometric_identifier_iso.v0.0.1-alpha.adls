definition
    Node[id1] matches {    -- Biometric identifier
        items matches {
            Node[id2] occurrences matches {1} matches {    -- Type of biometric
                value matches {
                    Coded_text[id11] matches {
                        term matches {
                            Terminology_term[id20] matches {
                                concept matches {[ac3]}    -- Type of biometric (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id3] occurrences matches {0..1} matches {    -- Biometric data template
                value matches {
                    Coded_text[id12] matches {
                        term matches {
                            Terminology_term[id21] matches {
                                concept matches {[ac1]}    -- Template code
                            }
                        }
                    }
                }
            }
            Node[id4] occurrences matches {0..1} matches {    -- Biometric enrolment quality
                value matches {
                    Ratio[id13]
                }
            }
            Node[id5] occurrences matches {0..1} matches {    -- Biometric creating authority
                value matches {
                    Text[id14]
                }
            }
            Node[id6] matches {    -- Location of biometric identifier
                value matches {
                    Coded_text[id15] matches {
                        term matches {
                            Terminology_term[id22] matches {
                                concept matches {[ac2]}    -- Localization code
                            }
                        }
                    }
                }
            }
            Node[id7] occurrences matches {0..1} matches {    -- Biometric schema
                value matches {
                    Text[id16]
                }
            }
            Node[id8] occurrences matches {0..1} matches {    -- Biometric device identification
                value matches {
                    Text[id17]
                }
            }
            Node[id9] occurrences matches {0..1} matches {    -- Biometric version
                value matches {
                    Text[id18]
                }
            }
            Node[id10] occurrences matches {0..1} matches {    -- Biometric date of creation
                value matches {
                    Date_value[id19]
                }
            }
        }
    }

