							}
						}
					}
				}
			}
			Node[id4] occurrences matches {0..1} matches {	-- Birth plurality
				value matches {
					Coded_text[id10] matches {
						term matches {
							Terminology_term[id14] matches {
								concept matches {[ac2]}		-- Birth plurality codes
							}
						}
					}
				}
			}
			Node[id5] occurrences matches {0..1} matches {	-- Birth order
				value matches {
					Coded_text[id11] matches {
						term matches {
							Terminology_term[id15] matches {
								concept matches {[ac3]}		-- Birth order codes
							}
						}
					}
				}
			}
			Node[id6] matches {	-- Date of birth follow-up indicator
				value matches {
					Boolean_value[id12] 
