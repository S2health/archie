definition
    Node[id1] matches {    -- Person identifier
        items matches {
            Node[id2] occurrences matches {1..*} matches {    -- Identifier main data
                value matches {
                    Rwe_id_ref[id8]
                }
            }
            Node[id3] occurrences matches {1..*} matches {    -- Geographic scope
                value matches {
                    Plain_text[id9]
                    Coded_text[id10] matches {
                        term matches {
                            Terminology_term[id18] matches {
                                concept matches {[ac5]}    -- Geographic scope (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id4] occurrences matches {0..1} matches {    -- Issuing City
                value matches {
                    Plain_text[id11]
                    Coded_text[id12] matches {
                        term matches {
                            Terminology_term[id19] matches {
                                concept matches {[ac2]}    -- City codes
                            }
                        }
                    }
                }
            }
            Node[id5] occurrences matches {0..1} matches {    -- Issuing State
                value matches {
                    Coded_text[id13] matches {
                        term matches {
                            Terminology_term[id20] matches {
                                concept matches {[ac3]}    -- State codes
                            }
                        }
                    }
                }
            }
            Node[id6] occurrences matches {0..1} matches {    -- Issuing Country
                value matches {
                    Coded_text[id14] matches {
                        term matches {
                            Terminology_term[id21] matches {
                                concept matches {[ac4]}    -- Country codes
                            }
                        }
                    }
                }
            }
            Node[id7] occurrences matches {0..1} matches {    -- Time validity
                value matches {
                    Range<Date_value>[id15] matches {
                        upper matches {
                            Date_value[id16]
                        }
                        lower matches {
                            Date_value[id17]
                        }
                    }
                }
            }
        }
    }

