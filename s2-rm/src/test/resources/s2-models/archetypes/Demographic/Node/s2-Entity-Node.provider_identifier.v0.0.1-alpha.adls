definition
    Node[id1] matches {    -- Healthcare provider identifier
        items cardinality matches {4..*; unordered} matches {
            Node[id2] occurrences matches {1} matches {    -- Identifier designation
                value matches {
                    Text[id26]
                }
            }
            Node[id3] occurrences matches {1} matches {    -- Geographic area
                value matches {
                    Coded_text[id27] matches {
                        term matches {
                            Terminology_term[id35] matches {
                                concept matches {[ac2]}    -- Geographic area (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id4] occurrences matches {1..*} matches {    -- Issuer
                value matches {
                    Text[id28]
                }
            }
            Node[id5] occurrences matches {1..*} matches {    -- Identifier Usage
                value matches {
                    Coded_text[id29] matches {
                        term matches {
                            Terminology_term[id36] matches {
                                concept matches {[ac1]}    -- Identifier usage code
                            }
                        }
                    }
                }
            }
            Node[id6] matches {    -- Duplicate resolution
                items cardinality matches {3..*; unordered} matches {
                    Node[id21] occurrences matches {0..1} matches {    -- Not a duplicate of
                        value matches {
                            Text[id30]
                        }
                    }
                    Node[id22] occurrences matches {0..1} matches {    -- Duplicate of
                        value matches {
                            Text[id31]
                        }
                    }
                    Node[id23] occurrences matches {1..*} matches {    -- Confirmed by organization
                        value matches {
                            Text[id32]
                        }
                    }
                    Node[id24] occurrences matches {1..*} matches {    -- Date confirmed
                        value matches {
                            Date_value[id33]
                        }
                    }
                    Node[id25] occurrences matches {1..*} matches {    -- Retired identifider
                        value matches {
                            Text[id34]
                        }
                    }
                }
            }
            allow_archetype Node[id7] matches {     -- Other data
                include
                    archetype_id/value matches {/s2-Entity-Node\.identifier[a-zA-Z0-9_-]*\.v0/}
            }
        }
    }

