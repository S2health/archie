definition
    Node[id1.1] matches {    -- Healthcare provider identifier
        /items matches {
            after [id7]
            Node[id0.2] occurrences matches {1..*} matches {    -- Identifier Usage
                value matches {
                    Plain_text[id0.35]
                    Coded_text[id0.36] matches {
                        term matches {
                            Terminology_term[id0.42] matches {
                                concept matches {[ac1.1]}    -- Identifier usage code
                            }
                        }
                    }
                }
            }
            Node[id0.3] matches {    -- Duplicate resolution
                items cardinality matches {3..*; unordered} matches {
                    Node[id0.30] occurrences matches {0..1} matches {    -- Not a duplicate of
                        value matches {
                            Text[id0.37]
                        }
                    }
                    Node[id0.31] occurrences matches {0..1} matches {    -- Duplicate of
                        value matches {
                            Text[id0.38]
                        }
                    }
                    Node[id0.32] occurrences matches {1..*} matches {    -- Confirmed by organization
                        value matches {
                            Text[id0.39]
                        }
                    }
                    Node[id0.33] occurrences matches {1..*} matches {    -- Date confirmed
                        value matches {
                            Date_value[id0.40]
                        }
                    }
                    Node[id0.34] occurrences matches {1..*} matches {    -- Retired identifider
                        value matches {
                            Text[id0.41]
                        }
                    }
                }
            }
        }
    }

