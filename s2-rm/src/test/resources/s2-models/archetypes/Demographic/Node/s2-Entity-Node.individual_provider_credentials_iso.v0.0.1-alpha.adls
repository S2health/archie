definition
    Node[id1] matches {    -- Individual healthcare provider credentials
        items matches {
            Node[id2] occurrences matches {1} matches {    -- Field of Practice
                value matches {
                    Coded_text[id16] matches {
                        term matches {
                            Terminology_term[id27] matches {
                                concept matches {[ac2]}    -- Field of practice codes
                            }
                        }
                    }
                }
            }
            Node[id3] occurrences matches {1} matches {    -- Primary field of practice
                value matches {
                    Boolean_value[id17]
                }
            }
            Node[id4] occurrences matches {1..*} matches {    -- Registration
                items matches {
                    Node[id6] occurrences matches {1} matches {    -- Registration ID
                        value matches {
                            Text[id18]
                        }
                    }
                    Node[id7] occurrences matches {1} matches {    -- Registering body
                        value matches {
                            Text[id19]
                        }
                    }
                    Node[id8] occurrences matches {1} matches {    -- Registration Status
                        value matches {
                            Coded_text[id20] matches {
                                term matches {
                                    Terminology_term[id28] matches {
                                        concept matches {[ac5]}    -- Registration Status (synthesised)
                                    }
                                }
                            }
                        }
                    }
                    Node[id9] occurrences matches {1} matches {    -- Period
                        value matches {
                            Range<Date_value>[id21]
                        }
                    }
                    use_archetype Node[id10, s2-Entity-Node.registration_other_data.v0] occurrences matches {0..1}     -- Other data
                }
            }
            Node[id5] matches {    -- Qualifications
                items matches {
                    Node[id11] occurrences matches {1} matches {    -- Qualification Name
                        value matches {
                            Text[id22]
                        }
                    }
                    Node[id12] occurrences matches {0..1} matches {    -- Qualification level
                        value matches {
                            Coded_text[id23] matches {
                                term matches {
                                    Terminology_term[id29] matches {
                                        concept matches {[ac3]}    -- Qualification level codes
                                    }
                                }
                            }
                        }
                    }
                    Node[id13] occurrences matches {1} matches {    -- Issuing Institution
                        value matches {
                            Text[id24]
                        }
                    }
                    Node[id14] occurrences matches {1} matches {    -- Issuing Institution Country
                        value matches {
                            Coded_text[id25] matches {
                                term matches {
                                    Terminology_term[id30] matches {
                                        concept matches {[ac4]}    -- Country codes
                                    }
                                }
                            }
                        }
                    }
                    Node[id15] occurrences matches {0..1} matches {    -- Qualification year
                        value matches {
                            Date_value[id26]
                        }
                    }
                }
            }
        }
    }

