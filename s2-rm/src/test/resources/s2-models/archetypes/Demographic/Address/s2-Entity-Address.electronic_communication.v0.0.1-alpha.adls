definition
    Address[id1] matches {    -- Electronic address
        name matches {"Telephone", "Fax", "Mobile", "Pager", "E-mail", "URL"}
        description matches {
            Node[id4] occurrences matches {0..1} matches {    -- Usage
                value matches {
                    Plain_text[id16]
                    Coded_text[id17] matches {
                        term matches {
                            Terminology_term[id20] matches {
                                concept matches {[ac2]}    -- Usage (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id5] occurrences matches {0..1} matches {    -- Time preferences
                value matches {
                    Plain_text[id18]
                    Coded_text[id19] matches {
                        term matches {
                            Terminology_term[id21] matches {
                                concept matches {[ac3]}    -- Time preferences (synthesised)
                            }
                        }
                    }
                }
            }
            Node[id8] occurrences matches {1} matches {    -- Details
                value matches {
                    Text[id13]     -- Identification of the electronic communication medium
                    Uri_ref[id10]     -- Internet address
                }
            }
        }
    }

