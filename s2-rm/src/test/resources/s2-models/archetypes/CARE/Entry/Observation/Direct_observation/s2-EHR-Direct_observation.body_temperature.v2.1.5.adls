definition
	Direct_observation[id1] matches {	-- Body temperature
		data cardinality matches {1..*; unordered} matches {
			Info_node[id5] matches {	-- Temperature
				value matches {
					Measured<Quantity>[id83] matches {
						value matches {
							Quantity[id77] matches {	-- Temperature (Cel)
								magnitude matches {|0.0..<100.0|}
								units matches {
									Coded_text[id79] matches {
										term matches {
											Terminology_term[id78] matches {
												concept matches {[at62]}
											}
										}
									}
								}
								precision matches {1}
							}
						}
					}
				}
				original_value matches {
					Measured<Quantity>[id84] matches {
						value matches {
							Quantity[id80] matches {	-- Temperature ([degF])
								magnitude matches {|30.0..<200.0|}
								units matches {
									Coded_text[id82] matches {
										term matches {
											Terminology_term[id81] matches {
												concept matches {[at63]}
											}
										}
									}
								}
								precision matches {1}
							}
						}
					}
				}
			}
			Info_node[id64] occurrences matches {0..1} matches {	-- Comment
				value matches {
					Text[id69] 
				}
			}
		}
		state matches {
			Info_node[id31] occurrences matches {0..1} matches {	-- Body exposure
				value matches {
					Coded_text[id70] matches {
						term matches {
							Terminology_term[id85] matches {
								concept matches {[ac1; at33]}		-- Body exposure (synthesised)
							}
						}
					}
