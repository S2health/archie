			}
			Info_node[id66] occurrences matches {0..1} matches {	-- Day of menstrual cycle
				value matches {
					Count[id73] matches {
						magnitude matches {|>=1|}
					}
				}
			}
			Info_node[id67] matches {	-- Confounding factors
				value matches {
					Text[id74] 
				}
			}
			use_archetype Info_node[id57, s2-EHR-Info_node.environmental_conditions.v0] 
