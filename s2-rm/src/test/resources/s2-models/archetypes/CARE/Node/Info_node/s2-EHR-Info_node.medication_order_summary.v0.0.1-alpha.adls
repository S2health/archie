    details = <
        ["en"] = <
            language = <[iso_639-1::en]>
            purpose = <"To provide an overall summary of the status and key dates, related to a medication order.">
            use = <"Use within the context of a medication order instruction where a summary of the overall order is required. This will normally be where the order is is being used within the context of a medicaton summary list, and not in the context of an orderable prescription record, where medication ctions will normally carry the primary record of the status of the order and key date information.">
            misuse = <"">
        >
    >
    
definition
    Info_node[id1] matches {    -- Medication order summary
        items matches {
            Info_node[id2] occurrences matches {0..1} matches {    -- Order status
                value matches {
                    Coded_text[id30] matches {
                        term matches {
                            Terminology_term[id34] matches {
                                concept matches {[ac1]}    -- Order status (synthesised)
                            }
                        }
                    }
                }
            }
            Info_node[id3] matches {    -- Key order dates
                name matches {"Date ordered/recommended", "Date first prescription issued", "Date last prescription issued", "Date first authorised", "Date last authorised", "Date first dispensed", "Date last dispensed", "Date first administered", "Date last administered", "Date discontinued", "Date authorised", "Date prescription issued", "Date dispensed", "Date administered", "Date administration withheld", "Date reviewed", "Date last reviewed", "Date changed"}
                value matches {
                    Date_time_value[id32]
                }
            }
            Info_node[id29] occurrences matches {0..1} matches {    -- Date reported
                value matches {
                    Date_time_value[id33]
                }
            }
        }
    }

