definition
	Entity_ref_node[id1] matches {	-- Reference to a Person
		items matches {
			Info_node[id2] occurrences matches {0..1} matches {	-- Name
				value matches {
					Plain_text[id102] 
				}
			}
			Info_node[id4] matches {	-- Identifier
				value matches {
					Rwe_id_ref[id104] 
				}
			}
			Info_node[id5] matches {	-- Relationship
				value matches {
					Coded_text[id105] matches {
						term matches {
							Terminology_term[id1102] matches {
								concept matches {[ac1]}		-- Relationship type
							}
						}
					}
				}
			}
		}
	}

