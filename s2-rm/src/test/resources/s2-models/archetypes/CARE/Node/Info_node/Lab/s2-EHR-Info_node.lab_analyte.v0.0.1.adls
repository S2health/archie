	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"Abstract parent lab analyte model">
		>
	>

definition
	Info_node[id1] matches {	-- Lab analyte result structure
		code matches {
			Terminology_term[id10] matches {
				concept matches {[at4]}
			}
		}
		items matches {
			Info_node[id3] matches {	-- Result status
				value matches {
					Coded_text[id4] matches {
						term matches {
							Terminology_term[id10] matches {
								concept matches {[ac1]}		-- Result status value set
							}
						}
					}
				}
			}
			Info_node[id5] matches {	-- Result time
				value matches {
					Date_time_value[id6] 
				}
			}
			Info_node[id8] occurrences matches {0..1} matches {
				value matches {
					Coded_text[id4] matches {
						term matches {
							Terminology_term[id10] matches {
								concept matches {[ac2]}
							}
						}
					}
				}
			}
			use_archetype Info_node[id9, s2-EHR-Info_node.specimen_details.v1] occurrences matches {0..1} 
		}
	}

