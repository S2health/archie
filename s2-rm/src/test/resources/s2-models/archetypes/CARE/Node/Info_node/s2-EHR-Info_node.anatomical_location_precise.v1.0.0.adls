definition
	Info_node[id1.1] matches {	-- Precise anatomical location
		items matches {
			Info_node[id0.2] occurrences matches {0..1} matches {	-- Position frame of reference
				value matches {
					Text[id0.6] 
				}
			}
			Info_node[id0.3] occurrences matches {0..1} matches {	-- X offset
				value matches {
					Quantity[id0.7] 
					Count[id0.8] matches {
						magnitude matches {|>=0|}
					}
				}
			}
			Info_node[id0.4] occurrences matches {0..1} matches {	-- Y offset
				value matches {
					Quantity[id0.9] 
					Count[id0.10] matches {
						magnitude matches {|>=0|}
					}
				}
			}
			Info_node[id0.5] occurrences matches {0..1} matches {	-- Z offset
				value matches {
					Quantity[id0.11] 
					Count[id0.12] matches {
						magnitude matches {|>=0|}
					}
				}
			}
		}
	}

