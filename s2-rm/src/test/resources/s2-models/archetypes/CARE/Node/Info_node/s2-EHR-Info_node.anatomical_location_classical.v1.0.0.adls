definition
    Info_node[id1.1]  matches {    -- Anatomical location
        items matches {
           Info_node[id0.2] matches {    -- Body site name
                value matches {
                    Text[id0.67]
                }
            }
            Info_node[id0.66] occurrences matches {0..1} matches {    -- Specific site
                value matches {
                    Text[id68]
                }
            }
            Info_node[id0.3] occurrences matches {0..1} matches {    -- Laterality
                value matches {
                    Coded_text[id0.69] matches {
                        term matches {
                            Terminology_term[id0.75] matches {
                                concept matches {[ac0.1]}    -- Laterality (synthesised)
                            }
                        }
                    }
                }
            }
            Info_node[id0.65] occurrences matches {0..2} matches {    -- Aspect
                value matches {
                    Coded_text[id0.70] matches {
                        term matches {
                            Terminology_term[id0.76] matches {
                                concept matches {[ac0.2]}    -- Aspect (synthesised)
                            }
                        }
                    }
                    Plain_text[id0.71]
                }
            }
            Info_node[id0.56] occurrences matches {0..1} matches {    -- Anatomical Line
                value matches {
                    Coded_text[id0.72] matches {
                        term matches {
                            Terminology_term[id0.77] matches {
                                concept matches {[ac0.3]}    -- Anatomical Line (synthesised)
                            }
                        }
                    }
                    Plain_text[id0.73]
                }
            }
        }
    }

