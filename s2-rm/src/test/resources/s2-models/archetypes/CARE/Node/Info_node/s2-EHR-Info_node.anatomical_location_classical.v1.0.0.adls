            >
            ["id0.65"] = <
                text = <"Aspect">
                description = <"Qualifying detail about the specific aspect of the identified body site.">
            >
            ["id0.66"] = <
                text = <"Specific site">
                description = <"Additional detail using a specific region or a point on, or within, the identified body site.">
            >
