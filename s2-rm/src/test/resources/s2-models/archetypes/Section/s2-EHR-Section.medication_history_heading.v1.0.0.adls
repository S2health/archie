			purpose = <"???">
		>
	>

definition
	Section[id1] matches {	-- -
		items matches {
			use_archetype  Order[id2, s2-EHR-Order.medication_order.v3]
			Section[id3] occurrences matches {1} matches {	-- Action history
				items matches {
					use_archetype Action[id4, s2-EHR-Action.medication.v1]
				}
			}
		}
	}

terminology
	term_definitions = <
		["en"] = <
			["id1"] = <
				text = <"Medication history">
				description = <"History of one medication">
			>
			["id2"] = <
				text = <"Medication order">
				description = <"The original medication order, if present">
			>
			["id3"] = <
				text = <"Action history">
				description = <"History of Actions performed to date for this order">
			>
			["id4"] = <
				text = <"Specific action">
				description = <"-">
			>
		>
	>
